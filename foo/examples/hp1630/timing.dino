file_format decsim

# Alias the signal names
signal_rename POD1-4 CLK
signal_rename POD1-0 DIV_2
signal_rename POD1-1 DIV_4
signal_rename POD1-2 DIV_8
signal_rename POD1-3 DIV_16

# Align grid to this signal
grid_signal	0	DIV_2

# Make the grid color stand out a bit more than default
#grid_color	0	4

# 500ns screen width
resolution 500

signal_delete *
signal_add CLK
signal_add DIV_2
signal_add DIV_4
signal_add DIV_8
signal_add DIV_16
